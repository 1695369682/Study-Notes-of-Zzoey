<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace:对应mapper的copy path/reference-->
<mapper namespace="cn.wolfcode.mapper.foodMapper">
<!--   select：sql标签 id：需要执行的方法的名 resultType：集合的类型 -->
    <select id="addFood" resultType="food">
-- sql语句
        select * from food
    </select>
</mapper>